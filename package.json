{
  "name": "andromeda-viewer",
  "version": "0.0.1",
  "private": true,
  "description": "Small web-client for connecting to the virtual world of Second Life.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Terreii/andromeda-viewer.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Christopher Astfalk",
    "email": "christopher.astfalk@icloud.com",
    "url": "http://christopher-astfalk.de/"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/Terreii/andromeda-viewer/issues"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:docs": "textlint --fix README.md ./*/*.md ./src/*/*.md",
    "lint:docs:fix": "textlint --fix README.md ./*/*.md ./src/*/*.md",
    "update-coc": "weallbehave -o . && git add CODE_OF_CONDUCT.md && git commit -m 'docs(coc): update CODE_OF_CONDUCT.md'",
    "update-contrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): update CONTRIBUTING.md'",
    "update-files": "node tools/updateFiles.js",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:server": "cross-env PORT=0 NODE_ENV=test mocha",
    "server:dev": "node backend/index.js",
    "server:start": "cross-env NODE_ENV=production node backend/index.js"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.0",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "cross-env": "^7.0.3",
    "double-ended-queue": "^2.1.0-0",
    "ember-auto-import": "^1.10.1",
    "ember-cli": "~3.25.3",
    "ember-cli-app-version": "^4.0.0",
    "ember-cli-babel": "^7.23.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.3.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-data": "~3.25.0",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.4",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.2",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.25.1",
    "ember-template-lint": "^2.18.1",
    "ember-welcome-page": "^4.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ember": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "hoodie-plugin-store-crypto": "^4.1.3",
    "jszip": "^3.5.0",
    "loader.js": "^4.7.0",
    "mocha": "^8.2.1",
    "mockdate": "^3.0.2",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "pouchdb-adapter-memory": "^7.2.2",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-browser": "^7.2.2",
    "pouchdb-hoodie-api": "^2.0.0",
    "pouchdb-server": "^4.2.0",
    "prettier": "^2.2.1",
    "proxyquire": "^2.1.3",
    "qunit": "^2.14.0",
    "qunit-dom": "^1.6.0",
    "sinon": "^9.2.1",
    "supertest": "^6.0.1",
    "textlint": "^11.7.6",
    "textlint-rule-alex": "^3.0.0",
    "textlint-rule-common-misspellings": "^1.0.1",
    "textlint-rule-rousseau": "^1.4.6",
    "weallbehave": "^1.2.0",
    "weallcontribute": "^1.0.9",
    "web-vitals": "^1.0.1"
  },
  "dependencies": {
    "basic-auth": "^2.0.1",
    "express": "^4.17.1",
    "express-validator": "^6.8.0",
    "http-proxy": "^1.18.1",
    "milliseconds": "^1.0.3",
    "nano": "^9.0.1",
    "node-fetch": "^2.6.1",
    "pouchdb-errors": "^7.2.2",
    "uuid": "^8.3.2",
    "ws": "^7.4.1",
    "xmlrpc": "^1.3.2"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  }
}
